{
  "name": "snippets-manager",
  "displayName": "Snippets Manager",
  "version": "0.0.1",
  "description": "Manage your own code snippets and turn your selection into a snippet easily!",
  "publisher": "promise96319",
  "repository": "https://github.com/promise96319/snippets-manager.git",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:snippets-manager.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snippets-manager.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prebuild": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
		"test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@qgh/eslint-config-ts": "^0.0.3",
    "@types/glob": "^7.2.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "typescript": "^4.5.5",
    "vitest": "^0.9.2"
  }
}
